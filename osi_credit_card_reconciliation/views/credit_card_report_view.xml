<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<!-- Qweb Reports -->
	    <report  
	        id="report_credit_card_detail" 
	        model="cc.rec.statement"
			string="Credit Card-Detail"
			report_type="qweb-pdf"
			name="osi_credit_card_reconciliation.credit_card_detail"
			file="osi_credit_card_reconciliation.credit_card_detail"
	        attachment="'Detail Statement-'+(object.name)"/>

	    <report  
	        id="report_credit_card_summary" 
	        model="cc.rec.statement"
			string="Credit Card-Summary"
			report_type="qweb-pdf"
			name="osi_credit_card_reconciliation.credit_card_summary"
			file="osi_credit_card_reconciliation.credit_card_summary"
	        attachment="'Summary Statement-'+(object.name)"/>
	</data>
</odoo>
